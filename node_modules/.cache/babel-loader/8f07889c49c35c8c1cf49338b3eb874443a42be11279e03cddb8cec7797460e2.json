{"ast":null,"code":"export default {\n  data() {\n    return {\n      products: [{\n        name: \"Product 1\",\n        price: 10\n      }, {\n        name: \"Product 2\",\n        price: 20\n      }, {\n        name: \"Product 3\",\n        price: 30\n      }],\n      quantities: [1, 1, 1]\n    };\n  },\n  methods: {\n    increaseQuantity(index) {\n      this.quantities[index]++;\n    }\n  },\n  computed: {\n    totalPrice(index) {\n      return this.products[index].price * this.quantities[index];\n    }\n  }\n};","map":{"version":3,"names":["data","products","name","price","quantities","methods","increaseQuantity","index","computed","totalPrice"],"sources":["/opt/lampp/htdocs/projects/vuejs/src/components/Product.vue"],"sourcesContent":["<template>\n  <div>\n    <div v-for=\"(product, index) in products\" :key=\"index\">\n      <h2>{{ product.name }}</h2>\n      <p>Price: ${{ product.price }}</p>\n      <p>Quantity: {{ quantities[index] }}</p>\n      <button @click=\"increaseQuantity(index)\">Increase Quantity</button>\n      <p>Total: ${{ totalPrice(index) }}</p>\n    </div>\n  </div>\n</template>\n\n<script>\nexport default {\n  data() {\n    return {\n      products: [\n        { name: \"Product 1\", price: 10 },\n        { name: \"Product 2\", price: 20 },\n        { name: \"Product 3\", price: 30 },\n      ],\n      quantities: [1, 1, 1],\n    };\n  },\n  methods: {\n    increaseQuantity(index) {\n      this.quantities[index]++;\n    },\n  },\n  computed: {\n    totalPrice(index) {\n      return this.products[index].price * this.quantities[index];\n    },\n  },\n};\n</script>\n"],"mappings":"AAaA,eAAe;EACbA,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,QAAQ,EAAE,CACR;QAAEC,IAAI,EAAE,WAAW;QAAEC,KAAK,EAAE;MAAG,CAAC,EAChC;QAAED,IAAI,EAAE,WAAW;QAAEC,KAAK,EAAE;MAAG,CAAC,EAChC;QAAED,IAAI,EAAE,WAAW;QAAEC,KAAK,EAAE;MAAG,CAAC,CACjC;MACDC,UAAU,EAAE,CAAC,CAAC,EAAE,CAAC,EAAE,CAAC;IACtB,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,gBAAgBA,CAACC,KAAK,EAAE;MACtB,IAAI,CAACH,UAAU,CAACG,KAAK,CAAC