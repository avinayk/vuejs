{"ast":null,"code":"import axios from \"axios\";\nexport default {\n  name: \"App\",\n  data() {\n    return {\n      names: [\"Hi\", \"Test\", \"Hello\"],\n      multimes: [{\n        name: \"Rk Verma\",\n        email: \"rk@gmail.com\",\n        phone: \"1325432546\"\n      }],\n      uname: \"\",\n      uemail: \"\",\n      ugender: \"\",\n      formSubmitted: false,\n      message: \"Submission Successfully! Please Login\",\n      showSuccessMessage: false\n    };\n  },\n  methods: {\n    submitForm() {\n      const requestOptions = {\n        method: \"POST\",\n        headers: {\n          \"Content-Type\": \"application/json\"\n        },\n        body: JSON.stringify({\n          email: this.uemail,\n          password: this.password\n        })\n      };\n      axios.post(\"http://localhost/projects/vuejs/formlogin.php\", requestOptions).then(response => {\n        console.log(response);\n        if (response.data == 1) {\n          this.errorMessage = true;\n          setTimeout(() => {\n            this.errorMessage = false;\n          }, 3500); // 2000 milliseconds = 2 seconds\n        } else {\n          this.uname = \"\";\n          this.uemail = \"\";\n          this.password = \"\";\n          this.address = \"\";\n          this.city = \"\";\n          this.state = \"\";\n          this.showSuccessMessage = true;\n          sessionStorage.setItem('users', response.data);\n          setTimeout(() => {\n            this.showSuccessMessage = false;\n          }, 3500); // 2000 milliseconds = 2 seconds\n        }\n      }).catch(error => {\n        console.error(error);\n      });\n    }\n  }\n};","map":{"version":3,"names":["axios","name","data","names","multimes","email","phone","uname","uemail","ugender","formSubmitted","message","showSuccessMessage","methods","submitForm","requestOptions","method","headers","body","JSON","stringify","password","post","then","response","console","log","errorMessage","setTimeout","address","city","state","sessionStorage","setItem","catch","error"],"sources":["/opt/lampp/htdocs/projects/vuejs/src/components/HelloWorld.vue"],"sourcesContent":["<template>\n<router-view></router-view>\n       <!-- ======= Contact Section ======= -->\n    <section id=\"contact\" class=\"contact\">\n      <div class=\"container\" data-aos=\"fade-up\">\n        <div class=\"row\">\n          <div class=\"col-md-4 offset-md-4\">\n\n  <div class=\"section-title\">\n          <h2>Login</h2>\n         \n        </div>\n\n        <div class=\"row\" data-aos=\"fade-up\" data-aos-delay=\"100\">\n\n         \n\n          <div v-if=\"showSuccessMessage\" class=\"alert alert-success\">\n            {{ message }}\n          </div>\n            <form @submit.prevent=\"submitForm\" class=\"php-email-form\">\n              <div class=\"row\">\n                <div class=\"col-md-12 form-group\">\n                  <input type=\"email\" class=\"form-control\" id=\"name\" \n              v-model=\"uemail\" placeholder=\"Your Name\" required>\n                </div>\n                <div class=\"col-md-12 form-group\">\n                  <input type=\"password\" class=\"form-control\"  v-model=\"password\" id=\"email\"  placeholder=\"Your Password\" required>\n                </div>\n              </div>\n              <div class=\"text-center\">\n                 \n                <button type=\"submit\">Login</button>\n                <router-link to=\"/register\" class=\"subb ml-2\">Sign Up</router-link>\n                \n              </div>\n            </form>\n         \n\n        </div>\n\n          </div>\n        </div>\n      \n\n      </div>\n    </section><!-- End Contact Section -->\n</template>\n\n<script>\n  import axios from \"axios\";\nexport default {\n  name: \"App\",\n  data() {\n    return {\n      names: [\"Hi\", \"Test\", \"Hello\"],\n      multimes: [\n        { name: \"Rk Verma\", email: \"rk@gmail.com\", phone: \"1325432546\" },\n      ],\n      uname: \"\",\n      uemail: \"\",\n      ugender: \"\",\n      formSubmitted: false,\n      message: \"Submission Successfully! Please Login\",\n      showSuccessMessage: false,\n    };\n  },\n  methods: {\n    submitForm() {\n      const requestOptions = {\n        method: \"POST\",\n        headers: { \"Content-Type\": \"application/json\" },\n        body: JSON.stringify({\n          \n          email: this.uemail,\n          password: this.password,\n          \n        }),\n      };\n      axios\n        .post(\"http://localhost/projects/vuejs/formlogin.php\", requestOptions)\n        .then((response) => {\n          console.log(response);\n          \n          if(response.data == 1){\n              this.errorMessage = true;\n\n            setTimeout(() => {\n              this.errorMessage = false;\n            }, 3500); // 2000 milliseconds = 2 seconds\n          } else {\n            this.uname = \"\";\n            this.uemail = \"\";\n            this.password = \"\";\n            this.address = \"\";\n            this.city = \"\";\n            this.state = \"\";\n            this.showSuccessMessage = true;\n            sessionStorage.setItem('users', response.data)\n\n            setTimeout(() => {\n              this.showSuccessMessage = false;\n            }, 3500); // 2000 milliseconds = 2 seconds\n          }\n        })\n        .catch((error) => {\n          console.error(error);\n        });\n    },\n  },\n};\n</script>\n\n<!-- Add \"scoped\" attribute to limit CSS to this component only -->\n<style scoped>\n.subb {\n    background: #e03a3c;\n    border: 0;\n    padding: 13px 32px;\n    color: #fff;\n    transition: 0.4s;\n    border-radius: 4px;\n}\nh3 {\n  margin: 40px 0 0;\n}\nul {\n  list-style-type: none;\n  padding: 0;\n}\nli {\n  display: inline-block;\n  margin: 0 10px;\n}\na {\n  color: #42b983;\n}\n</style>\n"],"mappings":"AAkDE,OAAOA,KAAI,MAAO,OAAO;AAC3B,eAAe;EACbC,IAAI,EAAE,KAAK;EACXC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,KAAK,EAAE,CAAC,IAAI,EAAE,MAAM,EAAE,OAAO,CAAC;MAC9BC,QAAQ,EAAE,CACR